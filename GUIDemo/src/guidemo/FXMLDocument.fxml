<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guidemo.FXMLDocumentController">
   <children>
      <HBox prefHeight="325.0" prefWidth="800.0" spacing="10.0">
         <children>
            <VBox prefHeight="295.0" prefWidth="195.0" spacing="20.0">
               <children>
                  <Label text="CheckBox Objects">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperoni" />
                  <CheckBox fx:id="pineappleCheckBox" mnemonicParsing="false" text="PineApple" />
                  <CheckBox fx:id="baconCheckBox" mnemonicParsing="false" text="Bacon" />
                  <Button mnemonicParsing="false" onAction="#pizzaOrderButtonPushed" text="Order Pizza" />
                  <Label fx:id="pizzaOrderLabel" alignment="TOP_LEFT" prefHeight="77.0" prefWidth="190.0" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="295.0" prefWidth="195.0" spacing="20.0">
               <children>
                  <Label text="Choicebox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxButtonPressed" text="Favourite Fruit" />
                  <Label fx:id="choiceBoxLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="295.0" prefWidth="195.0" spacing="20.0">
               <children>
                  <Label text="ComboBox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="comboBox" onAction="#comboBoxedWasUpdated" prefWidth="150.0" />
                  <Label fx:id="comboBoxLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="295.0" prefWidth="195.0" spacing="15.0">
               <children>
                  <Label text="RadioButton Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="phpRB" mnemonicParsing="false" onAction="#radioButtonedPushed" text="PHP" />
                  <RadioButton fx:id="javaRB" mnemonicParsing="false" onAction="#radioButtonedPushed" text="Java" />
                  <RadioButton fx:id="cSharpRB" mnemonicParsing="false" onAction="#radioButtonedPushed" text="C#" />
                  <RadioButton fx:id="pythonRB" mnemonicParsing="false" onAction="#radioButtonedPushed" text="Python" />
                  <Label fx:id="radioButtonLabel" text="Label" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox layoutY="313.0" prefHeight="275.0" prefWidth="789.0" spacing="25.0" AnchorPane.leftAnchor="0.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="Listview Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ListView fx:id="listView" prefHeight="250.0" prefWidth="200.0" />
               </children>
            </VBox>
            <StackPane prefHeight="143.0" prefWidth="173.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#copyStrings" text="Selected Gear -&gt;" />
               </children>
            </StackPane>
            <VBox prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="Textarea Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <TextArea fx:id="pcTextArea" prefHeight="250.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
